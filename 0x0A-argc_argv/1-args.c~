#include <stdio.h>
#include <stdlib.h>

/**
 * main - Prints # of arguments passed to the program.
 * @argv: array of pointers to program arguments. argv[0] being
 * the program name.
 * @argc: Number of arguments.
 * Return: 0.
 */

int main(int argc, char *argv[])
{
	(void)argv;
	int i, counter = 0;

	if(argc == 1)
		printf("%d\n", counter);

	for (i = 0; i < argc; i++)
	{
		counter += 1;
	}

	printf("%d\n", counter);
}
